{
  "manifest_version": 3,
  "name": "Get Bird Back",
  "version": "1.0",
  "description": "Replaces the ugly ùïè logo with the OG bird one.",
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "twitter-ico.png",
    "48": "twitter-ico.png",
    "128": "twitter-ico.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "bird.svg",
        "darkbird.svg",
        "twitter-ico.ico"
      ],
      "matches": [
        "https://*.twitter.com/*"
      ]
    }
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.twitter.com/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ]
}